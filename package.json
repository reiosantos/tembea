    "moment-timezone": "^0.5.23",
    "morgan": "^1.9.0",
    "node-schedule": "^1.3.1",
    "nodemailer": "^4.7.0",
    "pdfkit": "^0.9.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
