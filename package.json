{
  "name": "tembea",
  "version": "2.2.0",
  "description": "An application for managing trips at Andela",
  "main": "./src/index.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "tsc": "ts-node -r tsconfig-paths/register",
    "build": "yarn run clean && yarn run build-server && yarn run copy-static-assets",
    "build-server": "tsc -b tsconfig.build.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "start": "yarn tsc src/index.ts",
    "serve": "DEBUG=log node dist/src/index.js",
    "start:dev": "NODE_ENV=development DEBUG=log nodemon",
    "start:debug": "NODE_ENV=development DEBUG=log nodemon --config nodemon-debug.json",
    "lint": "tslint -c tslint.json -p tsconfig.json && yarn run lint-js",
    "lint-js": "node_modules/.bin/eslint ./src",
    "jest": "ts-node node_modules/jest/bin/jest.js",
    "jest:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest": "yarn lint && NODE_ENV=test yarn db:rollback && NODE_ENV=test yarn db:migrate yarn",
    "test": "yarn jest --no-cache  --detectOpenHandles --runInBand --forceExit",
    "watch-test": "yarn run test -- --watchAll",
    "sequelize": "ts-node node_modules/.bin/sequelize",
    "db:migrate": "yarn sequelize db:migrate",
    "db:rollback": "yarn sequelize db:migrate:undo:all",
    "db:rollmigrate": "yarn db:rollback && yarn db:migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela/tembea.git"
  },
  "keywords": [
    "trips",
    "travel"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andela/tembea/issues"
  },
  "homepage": "https://github.com/andela/tembea#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-classes": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@types/async": "^3.0.1",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "^0.0.35",
    "@types/dotenv": "^4.0.2",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.1.3",
    "@types/node": "^10.0.0",
    "@types/request": "^2.47.0",
    "@types/shelljs": "^0.8.5",
    "@types/supertest": "^2.0.4",
    "@types/validator": "^10.11.3",
    "@types/debug": "^4.1.4",
    "@types/faker": "^4.1.5",
    "axios-mock-adapter": "^1.15.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.8.0",
    "commitplease": "^3.2.0",
    "concurrently": "^3.5.1",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-staged": "^1.0.1",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "nock": "^10.0.2",
