  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "ignore": [],
    "linters": {
      "src/**/*.(ts|js)": [
        "yarn eslint-staged ./src --fix",
        "git add"
      ]
    }
  }
}
