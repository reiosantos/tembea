    "sequelize-cli": "^5.5.1",
    "sequelize-test-helpers": "^1.1.1",
    "shelljs": "^0.8.5",
    "sinon": "^7.3.2",
    "steno": "^0.4.4",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.4",
    "ts-node": "^5.0.0",
    "tsconfig-paths": "^3.7.0",
